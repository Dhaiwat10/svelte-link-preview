{
	"name": "svelte-link-preview",
	"version": "0.4.2",
	"license": "MIT",
	"private": false,
	"description": "Svelte library to preview links.",
	"author": "Magellan CL",
	"keywords": [
		"svelte",
		"svelte-link-preview",
		"link",
		"preview",
		"card"
	],
	"homepage": "https://github.com/magellancl/svelte-link-preview",
	"bugs": {
		"url": "https://github.com/magellancl/svelte-link-preview/issues"
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/magellancl/svelte-link-preview.git"
	},
	"scripts": {
		"dev": "vite dev",
		"build": "vite build",
		"preview": "vite preview",
		"check": "svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-check --tsconfig ./tsconfig.json --watch",
		"lint": "prettier --ignore-path .gitignore --check --plugin-search-dir=. .",
		"format": "prettier --ignore-path .gitignore --write --plugin-search-dir=. .",
		"package": "vite package"
	},
	"devDependencies": {
		"@sveltejs/kit": "^1.22.4",
		"prettier": "^2.4.1",
		"prettier-plugin-svelte": "^2.4.0",
		"svelte": "^3.42.6",
		"svelte-check": "^2.2.6",
		"svelte-preprocess": "^4.9.8",
		"svelte2tsx": "^0.4.10",
		"tslib": "^2.3.1",
		"typescript": "^4.4.3"
	},
	"exports": {
		".": {
			"types": "./dist/index.d.ts",
			"svelte": "./dist/index.js"
		},
	"files": ["dist"],
	"type": "module"
}
